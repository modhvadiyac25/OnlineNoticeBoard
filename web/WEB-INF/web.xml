<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>./template/student/login.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>student_login</servlet-name>
        <servlet-class>student.student_login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>download_notice</servlet-name>
        <servlet-class>student.download_notice</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>contact_servlet</servlet-name>
        <servlet-class>student.contact_servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>student_login</servlet-name>
        <url-pattern>/student_login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>download_notice</servlet-name>
        <url-pattern>/download_notice</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>contact_servlet</servlet-name>
        <url-pattern>/contact_servlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
